const int frameCount = sizeof(animationFrames) / sizeof(animationFrames[0]);
const int frameDelay = 100; 

int currentFrame = 0;

//Setup
void setup() {
  Serial.begin(115200);
  Wire.begin();

  if (!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("⚠️ Gagal terhubung ke OLED!"));
    while (1);
  }

  Serial.println(F("✅ OLED siap!"));
  display.clearDisplay(); 
}

//Loop
void loop() {
  display.clearDisplay();

  display.drawBitmap(
    0,
    0,
    animationFrames[currentFrame],
    SCREEN_WIDTH,
    SCREEN_HEIGHT,
    SSD1306_WHITE
  );

  display.display();
  currentFrame = (currentFrame + 1) % frameCount;
  delay(frameDelay);
}
